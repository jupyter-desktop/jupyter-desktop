{
  "name": "jupyter-desktop",
  "version": "1.0.1",
  "main": "electron/main.js",
  "private": true,
  "author": {
    "name": "BackcastPro",
    "email": "support@backcastpro.com"
  },
  "description": "jupyter-desktop Application",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:4200 && cross-env NODE_ENV=development electron .\"",
    "dev": "concurrently \"npm run serve\" \"npm run backend\"",
    "backend": "cd backend && set ENV=development && python run.py",
    "build": "npm run build:ng && npm run build:backend && npm run build:electron",
    "build:ng": "ng build --configuration production",
    "build:backend": "cd backend && pyinstaller app.spec",
    "build:electron": "cross-env NODE_ENV=production electron-builder",
    "build:clean": "if exist dist rmdir /s /q dist && if exist backend\\dist rmdir /s /q backend\\dist && if exist backend\\build rmdir /s /q backend\\build && if exist dist-electron rmdir /s /q dist-electron",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/fire": "^18.0.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@jupyterlab/services": "^7.1.0",
    "firebase": "^10.14.1",
    "lightweight-charts": "^4.1.3",
    "marked": "^12.0.0",
    "monaco-editor": "^0.54.0",
    "rxjs": "~7.8.0",
    "three": "^0.181.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.0",
    "@playwright/test": "^1.56.1",
    "@types/jasmine": "~5.1.0",
    "@types/three": "^0.181.0",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^29.4.6",
    "electron-builder": "^26.0.12",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "node-fetch": "^2.7.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.5.2",
    "wait-on": "^9.0.3",
    "ws": "^8.18.3"
  }
}
